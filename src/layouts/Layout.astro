--- 
import '../styles/reset.css';
import ElasticCustomCursor from "../components/subcomponents/ElasticCustomCursor.astro";

const { title } = Astro.props
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<slot />
    <ElasticCustomCursor />
	</body>
</html>
